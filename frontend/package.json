{
  "name": "todo-backend-ui",
  "version": "0.1.0",
  "description": "UI for Todo Backend",
  "main": "index.html",
  "scripts": {
    "start": "http-server dist",
    "build": "parcel build src/index.html",
    "copy:javascript": "rm -rf ../backend/javascript/static && cp -R dist ../backend/javascript/static",
    "copy:java": "rm -rf ../backend/java/src/main/resources/static && cp -R dist ../backend/java/src/main/resources/static",
    "dev": "parcel src/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vjcagay/todo-backend.git"
  },
  "author": "Vincent John Cagay",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/vjcagay/todo-backend/issues"
  },
  "homepage": "https://github.com/vjcagay/todo-backend#readme",
  "dependencies": {
    "@babel/runtime": "^7.12.0",
    "http-server": "^0.12.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "regenerator-runtime": "^0.13.7",
    "tailwindcss": "^1.8.13"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.12.0",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "parcel-bundler": "^1.3.1",
    "postcss": "^8.1.1"
